{
  "default": "multipass",
  "programs": {
    "cells": {
      "pipeline": {
        "frag": "cells.frag"
      },
      "uniforms": ["general", "webcam"]
    },
    "dots": {
      "pipeline": {
        "frag": "dots.frag"
      },
      "uniforms": ["general", "webcam"]
    },
    "glitch": {
      "pipeline": {
        "frag": "glitch.frag"
      },
      "uniforms": ["general", "audio_fft", "webcam"],
      "defaults": {
        "audio_fft_smoothing": 0.5
      }
    },
    "halftone": {
      "pipeline": {
        "frag": "halftone.frag"
      },
      "uniforms": ["general", "audio_fft", "webcam"],
      "defaults": {
        "audio_fft_smoothing": 0.95
      }
    },
    "multipass": {
      "pipeline": {
        "frag": "halftone.frag"
      },
      "uniforms": ["general", "audio_fft", "webcam", "multipass"],
      "defaults": {
        "audio_fft_smoothing": 0.95,
        "passes": 2
      }
    },
    "webcam": {
      "pipeline": {
        "frag": "webcam.frag"
      },
      "uniforms": ["general", "webcam"]
    }
  }
}
