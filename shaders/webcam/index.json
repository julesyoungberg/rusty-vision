{
  "default": "glitch",
  "programs": {
    "cells": {
      "pipeline": {
        "frag": "cells.frag"
      },
      "uniforms": ["general", "webcam"]
    },
    "dots": {
      "pipeline": {
        "frag": "dots.frag"
      },
      "uniforms": ["general", "webcam"]
    },
    "glitch": {
      "pipeline": {
        "frag": "glitch.frag"
      },
      "uniforms": ["general", "audio_fft", "webcam"],
      "defaults": {
        "audio_fft_smoothing": 0.5
      }
    },
    "webcam": {
      "pipeline": {
        "frag": "webcam.frag"
      },
      "uniforms": ["general", "webcam"]
    }
  }
}
